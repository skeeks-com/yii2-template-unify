# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = tab
indent_size = 2

[**.js]
indent_style = tab
indent_size = 2

[**.php]
indent_style = tab
indent_size = 2

[*.min.*]
indent_style = ignore
trim_trailing_whitespace = false
#insert_final_newline = ignore
